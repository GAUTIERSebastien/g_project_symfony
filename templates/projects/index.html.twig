{% extends 'base.html.twig' %}

{% block title %}{{ controller_name }}{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1 class="mt-4 mb-4">{{ controller_name }}</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <ul class="list-group">
            {% for project in projects %}
                <li class="list-group-item">
                    {{ project.title }}
                </li>
            {% endfor %}
            </ul>
        </div>
    </div>
</div>
{% endblock %}
